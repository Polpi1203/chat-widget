{
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs14.x",
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}